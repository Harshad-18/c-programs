#include <stdio.h>

int oddNumber(int,int)


int main() {
	int odd;
	int a[10]={2, 98, 99, 27, 31, 53, 88, 92, 11, 12};

	int num=oddNumber(a,10);

	printf("%d",num);




	return 0;
}
int oddNumber(int a[],int n){

	int odd=0,num;
	for(int i=0; i < n; i++){

		if(a[i] % 2 != 0){
		num=odd+a[i];
		}


	}
	return num;
}
